<script id="ejs_good_box" type="text/template">
    <div class="good">
        <div class="good_img">
            <img src="[%=list[0].icon%]" alt="">
        </div>
        <p class="good_detail">
            <span>简介:</span> [%=list[0].se%]
        </p>
    </div>
    <ul class="list">
        <li [%if(list[0].preWoman=="宜"){%]class=""[%}else if(list[0].preWoman=="忌"){%]class="no"[%}else{%]class="avoid"[%}%]>
            <div class="top ">孕妇[%=list[0].preWoman%]吃</div>
            <p>[%=list[0].pce%]</p>
        </li>
        <li [%if(list[0].mateWoman=="宜"){%]class=""[%}else if(list[0].mateWoman=="忌"){%]class="no"[%}else{%]class="avoid"[%}%]>
            <div class="top">坐月子[%=list[0].mateWoman%]吃</div>
            <p>[%=list[0].mce%]</p>
        </li>
        <li [%if(list[0].breastFeed=="宜"){%]class=""[%}else if(list[0].breastFeed=="忌"){%]class="no"[%}else{%]class="avoid"[%}%]>
            <div class="top">哺乳期[%=list[0].breastFeed%]吃</div>
            <p>[%=list[0].bce%]</p>
        </li>
        <li [%if(list[0].baby=="宜"){%]class=""[%}else if(list[0].baby=="忌"){%]class="no"[%}else{%]class="avoid"[%}%]>
            <div class="top">婴儿[%=list[0].baby%]吃</div>
            <p>[%=list[0].cce%]</p>
        </li>
    </ul>
    <div class="knowledge">
        <div class="top">
            <span>食用技巧</span>
        </div>
        <p>
            [%=list[0].tips%]
        </p>
    </div>
    <!--<div class="tool" id="tool">
        <div class="tool_top">
            推荐小工具
        </div>
        [%if(tools.length>0){%]
        <ul class="tool_list" id="tool_list">
            [%for(var i=0;i< tools.length;i++){%]
            <li class="tool">
                <a href="[%=tools[i].url%]">
                    <div class="img">
                        <img src="[%=tools[i].icon%]" alt="">
                    </div>
                    <div class="name">
                        [%=tools[i].title%]
                    </div>
                </a>
            </li>
            [%}%]
        </ul>
        [%}%]
    </div>-->
</script>
