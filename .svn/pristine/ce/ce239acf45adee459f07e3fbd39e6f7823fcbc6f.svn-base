                    <div class="animated ele_7">
                        <img src="image/index/pc/ani/7/communication_chat_two.png" alt="">
                    </div>
                    <div class="animated ele_8">
                        <img src="image/index/pc/ani/7/communication_chat_four.png" alt="">
                    </div>
                    <div class="animated ele_9">
                        <img src="image/index/pc/ani/7/communication_chat_one.png" alt="">
                    </div>
                </div>
            </div>
        </div>
        <div class="contentRight">
            <div class="contentRightImg">
                <img src="image/index/pc/phone.png" alt="" class="iphone">
                <div class="topHide">
                    <img src="image/index/pc/iphones/bg_0.png" alt="" class="animated phone_bg" id="phone_bg">
                </div>
                <div class="rightTips right2" style="display: none;">
                    <img src="image/index/pc/ani/2/record_right.png" alt="" class="animated tips">
                </div>
                <div class="rightTips right3" style="display: none;">
                    <img src="image/index/pc/ani/3/prepare_right_bubble_two.png" alt="" class="animated tips_1">
                    <img src="image/index/pc/ani/3/prepare_right_calendar_one.png" alt="" class="animated tips_2">
                </div>
                <div class="rightTips right4" style="display: none;">
                    <img src="image/index/pc/ani/4/quickening_two.png" alt="" class="animated board">
                    <img src="image/index/pc/ani/4/quickening_right_arrow.png" alt="" class="animated arrow">
                    <img src="image/index/pc/ani/4/quickening_right_one.png" alt="" class="animated barometer">
                </div>
                <div class="rightTips right6" style="display: none;">
                    <img src="image/index/pc/ani/6/tool_right_hand_one.png" alt="" class="animated hands">
                    <img src="image/index/pc/ani/6/tool_right_bubble_two.png" alt="" class="animated tips">
                    <img src="image/index/pc/ani/6/tool_right_bang_two.png" alt="" class="animated papa">
                </div>
                <div class="rightTips right7">
