        <div class="arrow arrowLeft" id="leftBtn">
            <div class="arrowImg"></div>
        </div>
        <div class="menus" id="menus">
            <ul>
                <li class="tab ">
                    <div class="tabImg">
                        <div class="img inoculation" data-normal="0px 0px" data-clicks="0px -100px" data-hovers="0px -50px"></div>
                    </div>
                    <div class="tabContent">孕育</div>
                </li>
                <li class="tab ">
                    <div class="tabImg">
                        <div class="img record" data-normal="-75px 0px" data-clicks="-75px -100px" data-hovers="-75px -50px"></div>
                    </div>
