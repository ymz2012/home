                    <div class="animated ele_2">
                        <img src="image/index/pc/ani/5/grow_scale_top.png" alt="" class="plate">
                        <div class="verLine"></div>
                        <img src="image/index/pc/ani/5/grow_scale_down.png" alt="" class="verLines">
                    </div>
                    <div class="animated ele_3">
                        <img src="image/index/pc/ani/5/grow_pointer.png" alt="">
                    </div>
                </div>
                <div class="animated fadeIn ani_6 ani" id="ani_6">
