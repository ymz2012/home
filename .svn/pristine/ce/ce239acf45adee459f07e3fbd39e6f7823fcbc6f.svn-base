                    <div class="animated ele_1_tag">
                        <img src="image/index/pc/ani/1/pregnat_tag_one.png" alt="">
                    </div>
                    <div class="animated ele_2">
                        <img src="image/index/pc/ani/1/pregnat_two.png" alt="">
                    </div>
                    <div class="animated ele_2_tag">
                        <img src="image/index/pc/ani/1/pregnat_tag_two.png" alt="">
                    </div>
                    <div class="animated ele_3">
                        <img src="image/index/pc/ani/1/pregnat_three.png" alt="">
                    </div>
                    <div class="animated ele_3_tag">
                        <img src="image/index/pc/ani/1/pregnat_tag_three.png" alt="">
                    </div>
                </div>
                <div class="animated fadeIn ani_2 ani" id="ani_2" >
