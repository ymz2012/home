                    <div class="animated ele_2">
                        <img src="image/index/pc/ani/3/prepare_calendar.png" alt="">
                    </div>
                    <div class="animated ele_3">
                        <img src="image/index/pc/ani/3/prepare_arrows_two.png" alt="">
                    </div>
                    <div class="animated ele_4 fadeInLeft">
                        <img src="image/index/pc/ani/3/prepare_star.png" alt="">
                    </div>
                    <div class="animated ele_5 fadeInRight">
                        <img src="image/index/pc/ani/3/prepare_germcell_two.png" alt="">
                    </div>
                    <div class="animated ele_6 rotateInUpLeft">
                        <img src="image/index/pc/ani/3/prepare_assay_one.png" alt="">
                    </div>
                    <div class="animated ele_7">
                        <span class="span1" id="timing_ani">75</span>
                        <span class="span2">%</span>
                    </div>
                </div>
                <div class="animated fadeIn ani_4 ani" id="ani_4">
                    <div class="animated ele_1">
                        <img src="image/index/pc/ani/4/quickening_leftfoot.png" class="leftFooter" alt="">
                        <img src="image/index/pc/ani/4/quickening_rightfoot.png" class="rightFooter" alt="">
                    </div>
                    <div class="animated ele_2">
                        <img src="image/index/pc/ani/4/quickening_add.png" alt="">
                    </div>
                </div>
                <div class="animated fadeIn ani_5 ani" id="ani_5" >
                    <div class="animated fadeInDown ele_1">
                        <img src="image/index/pc/ani/5/grow_boy.png" alt="">
