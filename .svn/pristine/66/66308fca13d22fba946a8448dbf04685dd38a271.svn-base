/*animate.css*/
.fadeInRightMiddle {                                             /*ani_1(孕育)左边动画*/
    -webkit-animation-name: fadeInRightMiddle;
    animation-name: fadeInRightMiddle;
}
@-webkit-keyframes fadeInRightMiddle {
    from{
        opacity:0;
        -webkit-transform: translate3d(200px, 0, 0);
        transform: translate3d(200px, 0, 0);
    }
    to{
        opacity: 1;
        -webkit-transform: none;
        transform: none;
    }
}
@keyframes fadeInRightMiddle {
    from {
        opacity: 0;
        -webkit-transform: translate3d(200px, 0, 0);
        transform: translate3d(200px, 0, 0);
    }
    to {
        opacity: 1;
        -webkit-transform: none;
        transform: none;
    }
}

.fadeInLeftMiddle {                                        /*ani_1(孕育)左边动画*/
    -webkit-animation-name: fadeInLeftMiddle;
    animation-name: fadeInLeftMiddle;
    animation-duration: 1.5s;
    -webkit-animation-duration: 1.5s;
}
@-webkit-keyframes fadeInLeftMiddle {
    from{
        opacity:0;
        -webkit-transform: translate3d(-200px, 0, 0);
        transform: translate3d(-200px, 0, 0);
    }
    to{
        opacity: 1;
        -webkit-transform: none;
        transform: none;
    }
}
@keyframes fadeInLeftMiddle {
    from{
        opacity:0;
        -webkit-transform: translate3d(-200px, 0, 0);
        transform: translate3d(-200px, 0, 0);
    }
    to{
        opacity: 1;
        -webkit-transform: none;
        transform: none;
    }
}

.runningRight{                    /*ani_2(记录)铅笔跑入动画*/
    -webkit-animation-name: runningRight;
    animation-name: runningRight;
    -webkit-animation-duration: 1s;
    animation-duration: 1s;
    -webkit-animation-iteration-count: infinite; /* 动画无限播放 */
    animation-iteration-count: infinite; /* 动画无限播放 */
    -webkit-animation-timing-function: step-start; /* 马上跳到动画每一结束桢的状态 */
    animation-timing-function: step-start; /* 马上跳到动画每一结束桢的状态 */
}
@-webkit-keyframes runningRight {
    0%{
        opacity: 0;
        -webkit-transform: translate3d(-200px, 0, 0);
        transform: translate3d(-200px, 0, 0);
    }
    5%{
        opacity: 1;
        background-position: 0px 0px;
    }
    20%{
        opacity: 1;
        background-position: -90px 0px;
    }
    35%{
        opacity: 1;
        background-position: 0px 0px;
    }
    60% {
        opacity: 1;
        background-position: -90px 0px;
    }

    75% {
        opacity: 1;
        background-position: 0px 0px;
    }
    90% {
        opacity: 1;
        background-position: -90px 0px;
    }
    100% {
        opacity: 1;
        background-position: 0px 0px;
    }
}
@keyframes runningRight {

    5% {
        opacity: 1;
        background-position: 0px 0px;
    }

    20% {
        background-position: -90px 0px;
    }

    35% {
        opacity: 1;
        background-position: 0px 0px;
    }

    60% {
        background-position: -90px 0px;
    }

    75% {
        background-position: 0px 0px;
    }
    90% {
        background-position: -90px 0px;
    }
    100% {
        opacity: 1;
        background-position: 0px 0px;
    }
}
.runnings {                       /*ani_2(记录)铅笔跑入动画*/
    /*-webkit-animation-name: runnings;*/
    /*animation-name: runnings;*/
    /*-webkit-animation-duration: 1.5s;*/
    /*animation-duration: 1.5s;*/
}
@-webkit-keyframes runnings {
    from {
        opacity: 0;
        -webkit-transform: translate3d(200px, 0, 0);
        transform: translate3d(200px, 0, 0);

    }

    to {
        opacity: 1;
    }

}
@keyframes runnings {
    from {
        opacity: 0;
        -webkit-transform: translate3d(200px, 0, 0);
        transform: translate3d(200px, 0, 0);

    }

    to {
        opacity: 1;
    }
}


@-webkit-keyframes circleProgressLoad_right {   /*ani_3(备孕)红圈动画*/
    0% {
        -webkit-transform: rotate(-135deg);
    }
    50%, 100% {
        -webkit-transform: rotate(45deg);
    }
}

@-moz-keyframes circleProgressLoad_right {
    0% {
        -moz-transform: rotate(-135deg);
    }
    50%, 100% {
        -moz-transform: rotate(45deg);
    }
}

@keyframes circleProgressLoad_right {
    0% {
        transform: rotate(-135deg);
    }
    50%, 100% {
        transform: rotate(45deg);
    }
}



@-webkit-keyframes circleProgressLoad_left {     /*ani_3(备孕)红圈动画*/
    0%, 50% {
        -webkit-transform: rotate(-135deg);
    }
    100% {
        -webkit-transform: rotate(-20deg);
    }
}

@-moz-keyframes circleProgressLoad_left {
    0%, 50% {
        -moz-transform: rotate(-135deg);
    }
    100% {
        -moz-transform: rotate(-20deg);
    }
}

@keyframes circleProgressLoad_left {
    0%, 50% {
        transform: rotate(-135deg);
    }
    100% {
