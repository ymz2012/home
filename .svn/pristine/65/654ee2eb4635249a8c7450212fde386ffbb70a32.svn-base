@import "../sassmixins/_mixins.scss";
@import "../cdn/myreset.min.css";
body{
  background:#f1f1f1;
}
.erro_box{
  width:px2rem-s(270px);
  height:px2rem-s(200px);
  position: absolute;
  top:50%;
  margin-top: px2rem-s(-100px);
  left:50%;
  margin-left:px2rem-s(-135px);
  text-align: center;
  .erro_img{
    width:px2rem-s(190px);
    height:px2rem-s(104px);
    margin: 0 auto px2rem-s(20px);
    img{
      width:100%;
      height:100%;
    }
  }
  p{
    color:#999;
    @include font-dpr(14px);
    @include font-normal();
    line-height: 1.5em;
  }
}